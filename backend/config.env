NODE_ENV=development
PORT=3000
MONGODB_URI=mongodb://localhost:27017/health_platform

JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=90d
JWT_COOKIE_EXPIRES_IN=90

EMAIL_USERNAME=your_email_username
EMAIL_PASSWORD=your_email_password
EMAIL_HOST=smtp.mailtrap.io
EMAIL_PORT=2525

AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=your_aws_region
AWS_BUCKET_NAME=your_s3_bucket_name

TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

CLIENT_URL=http://localhost:3000

# Video conferencing
VIDEO_API_KEY=your_video_api_key
VIDEO_API_SECRET=your_video_api_secret

# SMS gateway
SMS_API_KEY=your_sms_api_key
SMS_API_SECRET=your_sms_api_secret

# Push notifications
FIREBASE_PROJECT_ID=your_firebase_project_id
FIREBASE_PRIVATE_KEY=your_firebase_private_key
FIREBASE_CLIENT_EMAIL=your_firebase_client_email

# Rate limiting
RATE_LIMIT_WINDOW_MS=3600000
RATE_LIMIT_MAX=100

# File upload
MAX_FILE_SIZE=5242880
ALLOWED_FILE_TYPES=image/jpeg,image/png,application/pdf

# Security
BCRYPT_SALT_ROUNDS=12
PASSWORD_RESET_EXPIRES_IN=3600000
EMAIL_VERIFICATION_EXPIRES_IN=86400000

# Logging
LOG_LEVEL=debug
LOG_FORMAT=dev
LOG_DIR=logs

# Monitoring
SENTRY_DSN=your_sentry_dsn
NEW_RELIC_LICENSE_KEY=your_new_relic_license_key

# Cache
REDIS_URL=redis://localhost:6379
CACHE_TTL=3600

# Search
ELASTICSEARCH_NODE=http://localhost:9200
ELASTICSEARCH_USERNAME=elastic
ELASTICSEARCH_PASSWORD=your_elasticsearch_password

# Analytics
GOOGLE_ANALYTICS_ID=your_google_analytics_id
MIXPANEL_TOKEN=your_mixpanel_token

# Maps
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# OCR
OCR_API_KEY=your_ocr_api_key
OCR_API_SECRET=your_ocr_api_secret

# Payment
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret
PAYMENT_CURRENCY=USD

# Backup
BACKUP_SCHEDULE=0 0 * * *
BACKUP_RETENTION_DAYS=30
BACKUP_BUCKET=your_backup_bucket_name

# Maintenance
MAINTENANCE_MODE=false
MAINTENANCE_MESSAGE=System is under maintenance. Please try again later.

# Feature flags
ENABLE_TELEMEDICINE=true
ENABLE_PRESCRIPTION_SCANNER=true
ENABLE_BLOOD_DONOR_MATCHING=true
ENABLE_GEO_FENCED_ALERTS=true
ENABLE_MEDICATION_REMINDERS=true
ENABLE_EMERGENCY_SOS=true
ENABLE_HEALTH_RECORDS=true
ENABLE_APPOINTMENT_SCHEDULER=true 